{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "OneStepCloudAPI/OneStepCloudAPI.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "/*.yml" ],
        "cwd": "obj/api",
        "dest": "api"
      },
      {
        "files": [
          "index.md",
          "toc.md"
        ]
      }
    ],
    "dest": "wwwroot"
  }
}